<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">

<context:annotation-config/>
<bean id="bankPriceListener" class="priceListener.BankPriceListener"></bean>
<bean id="companyPriceListener" class="priceListener.CompanyPriceListener"></bean>
<bean id="bankProductData" class="entity.BankProductDataDB"></bean>
<bean id="cmpProductDataVal" class="service.CompanyProductDataValidationService"></bean>
</beans>
